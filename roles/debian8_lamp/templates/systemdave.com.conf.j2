# domain: systemdave.com
# public: /var/www/systemdave.com/public_html/

<VirtualHost *:80>
  # Admin email, Server Name (domain name), and any aliases
  ServerAdmin webmaster@systemdave.com
  ServerName  www.systemdave.com
  ServerAlias systemdave.com

  # Index file and Document Root (where the public files are located)
  DirectoryIndex index.html index.php
  DocumentRoot /var/www/systemdave.com/public_html
  # Log file locations
  LogLevel warn
  ErrorLog  /var/www/systemdave.com/log/error.log
  CustomLog /var/www/systemdave.com/log/access.log combined
</VirtualHost>
